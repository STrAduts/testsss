# 启动原有场地的Gazebo仿真（先不启动跟踪节点）
cd ~/ros2_ws && source install/setup.bash
ros2 launch rc2026_field rc2026_field_sim.launch.py

cd ~/ros2_ws && source install/setup.bash
ros2 run robot_follow_target camera_processor

cd ~/ros2_ws && source install/setup.bash
ros2 run robot_follow_target follow_controller

cd ~/ros2_ws
# 执行一键启动脚本（自动编译+加载环境+启动Gazebo+所有节点）
./src/robot_follow_target/scripts/run_follow.sh